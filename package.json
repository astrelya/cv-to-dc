  "scripts": {
    "build:all": "nx run-many --target=build --projects=cv-converter-web,cv-converter-api --parallel",
    "start:all": "nx run-many --target=serve --all --parallel",
    "start:front": "nx serve cv-converter-web",
    "start:api": "nx serve cv-converter-api",
